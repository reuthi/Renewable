      "type": "integer"
    },
    "azimuth": {
      "type": "integer"
    },
    "shading": {
      "type": "enumeration",
      "enum": [
        "none",
        "partial"
      ]
    },
    "surfaceMaterial": {
      "type": "string"
    },
    "surfaceRightsInAgreement": {
      "type": "string"
    },
    "typeOfSale": {
      "type": "enumeration",
      "enum": [
        "SealedBid",
        "NegotiatedSale"
      ]
