    "propertyType": {
      "type": "enumeration",
      "enum": [
        "solar",
        "wind",
        "hydro",
        "bio",
        "storage",
        "other"
      ]
    },
    "mw": {
      "type": "decimal"
    },
    "country": {
      "type": "string"
    },
    "percentOfDealForSale": {
      "type": "decimal"
    },
    "pictureUrl": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "salesPrice": {
      "type": "biginteger"
    },
    "assetStatus": {
      "type": "enumeration",
      "enum": [
        "operating",
        "development"
      ]
    },
    "description": {
      "type": "text"
    },
    "region": {
      "type": "string"
    },
    "ConstructionDate": {
      "type": "datetime"
    },
    "ComissioningDate": {
      "type": "datetime"
    },
    "sixMonthRevenue": {
      "type": "integer"
